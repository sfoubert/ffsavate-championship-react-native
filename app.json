{
  "expo": {
    "name": "FFSavate Championship",
    "slug": "ffsavate-championship-react-native",
    "description": "FFSavate Championship is an app written in react-native",
    "githubUrl": "https://github.com/sfoubert/ffsavate-championship-react-native",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.ffsavate.championship",
      "versionCode": 10009,
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "bundleIdentifier": "com.ffsavate.championship",
      "buildNumber": "2.0.3",
      "supportsTablet": true
    }
  }
}
